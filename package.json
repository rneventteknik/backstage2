        "react-dom": "^17.0.2",
        "react-highlight-words": "^0.18.0",
        "react-loading-skeleton": "^3.0.2",
        "react-markdown": "^8.0.3",
        "remark-gfm": "^3.0.1",
        "sass": "^1.32.12",
