{
  "name": "backstage2",
  "version": "0.1.0-alpha",
  "scripts": {
    "dev": "next",
    "postinstall": "husky install",
    "build": "next build",
    "start": "next start",
    "tc": "tsc",
    "lint": "eslint \"*/**/*.{js,ts,tsx}\"",
    "tcl": "tsc && eslint \"*/**/*.{js,ts,tsx}\"",
    "migrate": "knex --esm migrate:latest",
    "migrate:down": "knex --esm migrate:down",
    "seed": "knex --esm seed:run",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(scss|sass|css)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@react-pdf/renderer": "^2.0.16",
    "@types/react-bootstrap-typeahead": "^5.1.4",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.6.0",
    "dotenv": "^10.0.0",
    "knex": "^0.21.19",
    "next": "^10.1.3",
    "next-iron-session": "^4.1.12",
    "objection": "^2.2.15",
    "pg": "^8.5.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-bootstrap-typeahead": "^5.1.4",
    "react-bus": "^3.0.0",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^3.0.2",
    "sass": "^1.32.12",
    "swr": "^0.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.38",
    "@types/react-bootstrap-typeahead": "^5.1.4",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.5",
    "msw": "^0.30.0",
    "next-page-tester": "^0.26.1",
    "prettier": "^2.2.1",
    "sqlite3": "^5.0.2",
    "typescript": "^4.2.4",
    "whatwg-fetch": "^3.6.2"
  },
  "license": "MIT"
}
