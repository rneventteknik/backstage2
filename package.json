        "@googleapis/calendar": "^3.0.0",
        "@googleapis/drive": "^3.0.0",
        "@react-pdf/renderer": "^2.0.16",
        "@slack/web-api": "^6.8.1",
        "archiver": "^5.3.1",
        "bcryptjs": "^2.4.3",
        "better-sqlite3": "^8.0.1",
        "bootstrap": "^4.0.0",
        "dotenv": "^16.0.0",
        "iconv-lite": "^0.6.3",
        "iron-session": "^6.0.5",
        "knex": "^2.3.0",
        "next": "^12.1.0",
        "objection": "^3.0.1",
        "pg": "^8.5.1",
